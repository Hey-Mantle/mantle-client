{
  "name": "@heymantle/client",
  "version": "1.1.69",
  "description": "Mantle API Client for TypeScript and JavaScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build-and-publish": "npm install && npm run build && npm publish"
  },
  "author": "jnormore",
  "license": "ISC",
  "publishConfig": {
    "@heymantle:registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hey-Mantle/mantle-client.git"
  },
  "bugs": {
    "url": "https://github.com/Hey-Mantle/mantle-client/issues"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "typescript": "^5.4.3",
    "tsup": "^8.0.2",
    "vitest": "^1.2.0"
  }
}
